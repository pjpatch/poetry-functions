def poemtest2():
    title=str.upper(r(adjectives)+' '+r(objects))
    print(title)
    a=meter2('trochaic',3,3,adverbs,adverbs,adverbs)
    for j in range(4):
        print(meter2('iambic',3,3,adjectives,subjects,verbs))
        print(meter2('trochaic',4,3,adverbs,subjects,verbs,objects))
        print(meter2('trochaic',4,3,subjects,verbs,adjectives,objects))
        if j%3==0:
            print(a,'\n')
        else:
            print(meter2('trochaic',3,3,adverbs,adverbs,adverbs),'\n')
            
